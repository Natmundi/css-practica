<html>
  <head>
    <title>
      Dashboard page
    </title>
    <meta charset='utf-8' />
    <link
      rel='apple-touch-icon'
      sizes='180x180'
      href='/fav/apple-touch-icon.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='32x32'
      href='/fav/favicon-32x32.png'
    />
    <link
      rel='icon'
      type='image/png'
      sizes='16x16'
      href='/fav/favicon-16x16.png'
    />
    <link rel='manifest' href='/fav/site.webmanifest' />
    <meta
      name='msapplication-TileColor'
      content='#da532c'
    />
    <meta name='theme-color' content='#ffffff' />

    <link href='css/normalize.css' rel='stylesheet' />

    <link href='css/dashboard.css' rel='stylesheet' />

    <meta
      name='viewport'
      content='viewport-fit=cover, width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no'
    />
  </head>

  <body>
    <div class='page page--dashboard'>
      <aside class='page__aside aside vertical'>
        <div class='text aside__logo'>Sidebar</div>
        <nav class="vertical vertical--small">
          <a
            href='#'
            class='nav-button nav-button__active click'
          >
            <span
              class='icon nav-button__icon'
              style='background-image: url(/svg/nav/home.svg)'
            ></span>
            <span class='text text--light'>Home</span>

             <a
            href='#'
            class='nav-button click'
          >
            <span
              class='icon nav-button__icon'
              style='background-image: url(/svg/nav/orders.svg)'
            ></span>
            <span class='text text--light'>Orders</span>

              <a
            href='#'
            class='nav-button click'
          >
            <span
              class='icon nav-button__icon'
              style='background-image: url(/svg/nav/products.svg)'
            ></span>
            <span class='text text--light'>Products</span>

          </a>
        <div class="relative">
          
            <div class="field field--select">
              <span class="text text--light">Choose...</span>
             <span class="icon select--icon-check" style="background-image: url(/svg/select/check.svg)"></span> 
          </div>
          <select class="field__select click" name="" id="">
                <option value="dog">Dog</option>
                <option value="cat">Cat</option>
                <option value="parrot">Parrot</option>
                <option value="hamster">Hamster</option>
                <option value="bird">Bird</option>
              </select>
        </div>
        </nav>
      </aside>
      <header class='header'>
        <div class="layout header__content text--small">
          <div class="header__search relative">
            <input type="text" placeholder="Search" class="text field field--header">
            <span class="icon header__icon-search" style="background-image: url(/svg/header/search.svg)"></span>
            <span class="icon icon--header-settings click" style="background-image: url(/svg/header/settings.svg)"></span>
          </div>
          <div class="icon__list">
              <span class="icon icon--header-size click" style="background-image: url(/svg/header/antena.svg)"></span>
              <span class="icon icon--header-size click" style="background-image: url(/svg/header/chinasign.svg)"></span>
              <span class="icon icon--header-size click" style="background-image: url(/svg/header/bell.svg)"></span>
          </div>
        </div>
      </header>
      <main class='main'>
        <div class="layout">
          <section class="box analytic__block">
          <div class="analytic analytic--double analytic--purple">
            <span class="icon analytic__button click" style="background-image: url(/svg/analytic/earn.svg)"></span>

            <div class="relative vertical vertical--small">
              <div class="analytic__title text text--light">$500.00</div>
              <div class="text text--small text--lighter">Total earning</div>

               <span class="icon analytic__arrow click" style="background-image: url(/svg/analytic/purple-arrow.svg)"></span>
            </div>
          </div>

          <div class="analytic analytic--double analytic--blue">
            <span class="icon analytic__button icon--button click" style="background-image: url(/svg/analytic/order.svg)"></span>

            <div class="relative vertical vertical--small">
              <div class="analytic__title text text--light">$961</div>
              <div class="text text--small text--lighter">Total Order</div>
            </div>
          </div>

          <div class="analytic analytic--blue">
            
            <div class="relative horizontal">
              <span class="icon analytic__button click" style="background-image: url(/svg/analytic/earn.svg)"></span>
              <div class="analytic--horizontal">
                <div class="analytic__title analytic__title--small text text--light">$203k</div>
                <div class="text text--small text--lighter">Total Income</div>
              </div>
            </div>
          </div>

          <div class="analytic analytic--white">
            <div class="relative horizontal">
              <span class="icon analytic__button click" style="background-image: url(/svg/analytic/income.svg)"></span>
                <div class="analytic--horizontal">
                  <div class="analytic__title analytic__title--small text text--bold">$203k</div>
                  <div class="text text--gray text--small">Total Income</div>
                </div>
            </div>
          </div>
        </section>

        <main class="main__2">
          <section class="purchase__block box vertical">
            <h3 class="purchase__title text text--heading">Purchase list</h3>
            <hr class="divider">

            <div class="purchase__list vertical">

              <div class="purchase__content">
                <span class="text">ID437661</span>
                <span class="text">May 11, 2023, 14:21</span>
              </div>
              <div class="purchase__content">
                <span class="text">AirOn ProCam</span>
                <span class="text">7/8 USB Type-C</span>
              </div>
              <div class="purchase__content">
                <span class="text">132.32 EUR</span>
                <span class="text text--warning">Not paid</span>
              </div>
              <div class="purchase__content">
                <span class="text">Customer</span>
                <span class="text">robin@com.de</span>
              </div>

            </div>

            <hr class="divider">

            <div class="purchase__list vertical">
                <div class="purchase__content">
                  <span class="text">ID437661</span>
                  <span class="text">May 11, 2023, 14:21</span>
                </div>
                <div class="purchase__content">
                  <span class="text">PHILIPS Essential</span>
                  <span class="text">HD9200/90</span>
                </div>
                <div class="purchase__content">
                  <span class="text">543 USD</span>
                  <span class="text text--succes">Pay by  Stripe</span>
                </div>
                <div class="purchase__content">
                  <span class="text">Customer</span>
                  <span class="text">dima@com.ua</span>
                </div>
            </div>
          </section>

          <section class="box">
            <div class="payment__block vertical">
              <h3 class="purchase__title text text--heading">Payment</h3>
              <hr class="divider">

              <div class="checkbox__block">
                <div class="checkbox__content">
                  <input type="radio" class="checkbox__input">
                  <label for="" class="text">Credit card</label>
                </div>
                <div class="checkbox__content">
                  <input type="radio" class="checkbox__input">
                  <label for="" class="text">Debit card</label>
                </div>
                <div class="checkbox__content">
                  <input type="radio" class="checkbox__input">
                  <label for="" class="text">PayPal</label>
                </div>
              </div>

              <div class="input__block vertical">
                <div class="input__content">
                  <span class="text">Name of card</span>
                  <input type="text" class="input">
                </div>
                <div class="input__content">
                  <span class="text">Credit card number</span>
                  <input type="text" class="input">
                </div>
                <div class="input__content">
                  <span class="text">Expiration</span>
                  <input type="text" class="input">
                </div>
                <div class="input__content">
                  <span class="text">CVV</span>
                  <input type="text" class="input">
                </div>
            </div>

            <hr class="divider">
              <div class="credit__block">
                <div class="credit__content">
                  <span class="text">Country</span>
                    <div class="relative">
            
                        <div class="field field--select">
                          <span class="text text--black">Choose...</span>
                          <span class="icon select--icon-check" style="background-image: url(/svg/select/check.svg)"></span> 
                        </div>

                        <select class="field__select click" name="" id=""></select>
                    </div>
                </div>
                <div class="credit__content">
                  <span class="text">State</span>
                    <div class="relative">
            
                        <div class="field field--select">
                          <span class="text text--black">Choose...</span>
                          <span class="icon select--icon-check" style="background-image: url(/svg/select/check.svg)"></span> 
                        </div>

                        <select class="field__select click" name="" id=""></select>
                    </div>
                </div>
              </div>

              <hr class="divider">
            <div class="button__block">
                  <button class="button click click:hover">Continue to checkout</button>
            </div>
            </div>
          </section>

        </main>
        </div>
      </main>
    </div>
  </body>

</html>